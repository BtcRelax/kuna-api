{
    "name": "madmis/kuna-api",
    "description": "Kuna.io REST API php client",
    "keywords": [
        "kuna.io",
        "kuna",
        "rest",
        "api"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/annotations": "^1.4",
        "symfony/options-resolver": "^3.3",
        "jms/serializer": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "madmis\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/console": "^3.3"
    }
}
